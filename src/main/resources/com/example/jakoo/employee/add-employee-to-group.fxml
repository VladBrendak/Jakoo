<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.jakoo.controller.group.AddEmployeeToGroupController">
   <children>
      <Label fx:id="groupNameLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="26.0" layoutY="23.0" prefHeight="27.0" prefWidth="220.0" text="Group Name">
         <font>
            <Font size="18.0" />
         </font></Label>
      <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#goBack" text="EXIT" />
      <StackPane layoutX="352.0" layoutY="50.0" prefHeight="300.0" prefWidth="220.0">
         <children>
            <ScrollPane fx:id="addEmployeeToGroupScrollPane" prefHeight="300.0" prefWidth="300.0">
               <content>
                  <FlowPane fx:id="addEmployeeToGroupFlowPane" prefHeight="291.0" prefWidth="218.0" rowValignment="TOP" />
               </content>
            </ScrollPane>
         </children>
      </StackPane>
      <StackPane layoutX="26.0" layoutY="50.0" prefHeight="300.0" prefWidth="220.0">
         <children>
            <ScrollPane fx:id="GroupMembersScrollPane" prefHeight="300.0" prefWidth="300.0">
               <content>
                  <FlowPane fx:id="GroupMembersFlowPane" prefHeight="291.0" prefWidth="218.0" rowValignment="TOP" />
               </content>
            </ScrollPane>
         </children>
      </StackPane>
      <Button layoutX="534.0" layoutY="361.0" mnemonicParsing="false" onAction="#saveGroupMembers" text="Save" />
      <Label layoutX="393.0" layoutY="23.0" prefHeight="27.0" prefWidth="139.0" text="Other Employees">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
